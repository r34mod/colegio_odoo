<?xml version="1.0" encoding="utf-8"?>
<odoo>


        
    <record model="ir.ui.view" id="view_colegio_materiales_tree">
        <field name="name">view.colegio.materiales.tree</field>
        <field name="model">colegio.materiales</field>
        <field name="arch" type="xml">
            <tree string="Idea list">
              <field name="centroMaterial"/>
              <field name="profesor"/>
              <field name="nombreAlumno"/>
              <field name="apellidoAlumno"/>
              <field name="nacimientoAlumno"/>
              <field name="edadAlumno"/>
              <field name="genero"/>
            </tree>
        </field>
    </record>

        
        <record model="ir.ui.view" id="view_colegio_materiales_form">
            <field name='name'>view.colegioo.materiales.form</field>
            <field name="model">colegio.materiales</field>
            <field name="arch" type="xml">
                <form string="materiales form" version="7.0">
                 <group colspan="2" col="2">
                        <field name="centroMaterial"/>
                        <field name="profesor"/>
                        <field name="nombreAlumno"/>
                         <field name="apellidoAlumno"/>
                         <field name="nacimientoAlumno"/>
                         <field name="edadAlumno"/>
                        <field name="genero"/>
                 </group>
                </form>
            </field>
        </record>
</odoo>